<?xml version="1.0" encoding="UTF-8"?><web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Production</param-value>
    </context-param>
    <context-param>
        <param-name>DisableInjection</param-name>
        <param-value>false</param-value>
    </context-param>
    <context-param>
        <param-name>EnableHttpSessionWrapper</param-name>
        <param-value>N</param-value>
    </context-param>
    <filter>
        <filter-name>HttpSessionWrapperFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.HttpSessionWrapperFilter</filter-class>
    </filter>
    <filter>
        <filter-name>SECRequestValidator</filter-name>
        <filter-class>com.newgen.ibps.security.xss.SECRequestValidator</filter-class>
        <init-param>
            <param-name>ModuleName</param-name>
            <param-value>psiform</param-value>
        </init-param>
        <init-param>
            <param-name>WhiteListMode</param-name>
            <param-value>P</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>IFormClickJackFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.IFormClickJackFilter</filter-class>
    </filter>
    <filter>
        <filter-name>SECCSRFValidator</filter-name>
        <filter-class>com.newgen.ibps.security.csrf.SECCSRFValidator</filter-class>
        <init-param>
            <param-name>ModuleName</param-name>
            <param-value>psiform</param-value>
        </init-param>
        <init-param>
            <param-name>SessionTokenName</param-name>
            <param-value>WD_SID</param-value>
        </init-param>
        <init-param>
            <param-name>RequestTokenName</param-name>
            <param-value>WD_RID</param-value>
        </init-param>
        <init-param>
            <param-name>CSRFMode</param-name>
            <param-value>SR</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>UUIDFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.UUIDFilter</filter-class>
    </filter>
    <filter>
        <filter-name>SessionCheckFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.SessionCheckFilter</filter-class>
        <init-param>
            <param-name>EnableStripXSSChecks</param-name>
            <param-value>Y</param-value>
        </init-param>
        <init-param>
            <param-name>EnableCSP</param-name>
            <param-value>N</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>SessionTimeCheckFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.SessionTimeCheckFilter</filter-class>
        <init-param>
            <param-name>AllowedOrigin</param-name>
            <param-value>*</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>ResponseHeaderFilter</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.ResponseHeaderFilter</filter-class>
        <init-param>
            <param-name>Cache-Control</param-name>
            <param-value>max-age=3600</param-value>
        </init-param>
        <init-param>
            <param-name>Cache-Time</param-name>
            <param-value>3600</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>InputUserSecurity</filter-name>
        <filter-class>com.newgen.iforms.servlet.filter.InputUserSecurity</filter-class>
        <init-param>
            <param-name>BurpInjection</param-name>
            <param-value>|ping,| ping,&amp;ping,&amp; ping,&amp;&amp;ping,`ping,||ping,%0aping,%0a ping,%0a%0dping,sleep(,waitfor delay,receive_message,eval(,system(,compile(,xmltype(,admin' #,' or 1=1--,' or 1=1#,') or '1'='1--,') or ('1'='1--,' HAVING 1=1 --,' and '</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>HttpSessionWrapperFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>IFormClickJackFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
        <url-pattern>/GetReqToken</url-pattern>
        <url-pattern>/IFormSessionToken</url-pattern>
        <url-pattern>/secuidsv</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SECCSRFValidator</filter-name>
        <url-pattern>/IFormSessionToken</url-pattern>
        <url-pattern>*.jsf</url-pattern>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>InputUserSecurity</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SECRequestValidator</filter-name>
        <url-pattern>*.jsf</url-pattern>
        <url-pattern>*.jsp</url-pattern>
        <url-pattern>/ClearTemp</url-pattern>
        <url-pattern>/UploadFile</url-pattern>
        <url-pattern>/UploadFrame</url-pattern>
        <url-pattern>/UploadControl</url-pattern>
        <url-pattern>/GetImage</url-pattern>
        <url-pattern>/DownloadFrame</url-pattern>
        <url-pattern>/UploadWebService</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SessionCheckFilter</filter-name>
        <url-pattern>*.jsf</url-pattern>
        <url-pattern>/UploadFile</url-pattern>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SessionTimeCheckFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>UUIDFilter</filter-name>
        <url-pattern>*.jsf</url-pattern>
        <url-pattern>/components/viewer/viewform.jsp</url-pattern>
        <url-pattern>/UploadFile</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>ResponseHeaderFilter</filter-name>
        <url-pattern>*.gif</url-pattern>
        <url-pattern>*.jpg</url-pattern>
        <url-pattern>*.png</url-pattern>
        <url-pattern>*.js</url-pattern>
        <url-pattern>*.css</url-pattern>
        <url-pattern>*.ico</url-pattern>
        <url-pattern>/resources/images/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ClearCache</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.ClearCache</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClearTemp</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.ClearTemp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetImage</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.GetImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadFrame</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.DownloadFrame</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormGetSessionTime</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.FormGetSessionTime</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormUpdateSessionTime</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.FormUpdateSessionTime</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadPDF</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.DownloadPDF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadWebService</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.UploadWebService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocumentUpload</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.DocumentUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>docDownload</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.docDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IFormSessionToken</servlet-name>
        <servlet-class>com.newgen.iforms.session.IFormSessionToken</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetReqToken</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.GetReqToken</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFrame</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.UploadFrame</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadControl</servlet-name>
        <servlet-class>com.newgen.iforms.servlet.UploadControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClearCache</servlet-name>
        <url-pattern>/ClearCache</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadPDF</servlet-name>
        <url-pattern>/DownloadPDF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>docDownload</servlet-name>
        <url-pattern>/docDownload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClearTemp</servlet-name>
        <url-pattern>/ClearTemp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFrame</servlet-name>
        <url-pattern>/UploadFrame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadControl</servlet-name>
        <url-pattern>/UploadControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetImage</servlet-name>
        <url-pattern>/GetImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadFrame</servlet-name>
        <url-pattern>/DownloadFrame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormGetSessionTime</servlet-name>
        <url-pattern>/getsessiontime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormUpdateSessionTime</servlet-name>
        <url-pattern>/updatesessiontime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadWebService</servlet-name>
        <url-pattern>/UploadWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocumentUpload</servlet-name>
        <url-pattern>/DocumentUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IFormSessionToken</servlet-name>
        <url-pattern>/IFormSessionToken</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetReqToken</servlet-name>
        <url-pattern>/GetReqToken</url-pattern>
    </servlet-mapping>
    <session-config/>
    <welcome-file-list>
        <welcome-file>components/viewer/portal/redirector.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>403</error-code>
        <location>/errorpage.html</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/errorPage404.html</location>
    </error-page>
    <error-page>
        <exception-type>java.lang.Exception</exception-type>
        <location>/errorPageCustom.html</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/errorPageCustom.html</location>
    </error-page>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>DisabledMethods</web-resource-name>
            <url-pattern>/*</url-pattern>
            <http-method>PUT</http-method>
            <http-method>TRACE</http-method>
            <http-method>DELETE</http-method>
            <http-method>OPTIONS</http-method>
        </web-resource-collection>
        <auth-constraint/>
    </security-constraint>
    <post-construct>
        <lifecycle-callback-class>com.newgen.iforms.Download</lifecycle-callback-class>
        <lifecycle-callback-method>init</lifecycle-callback-method>
    </post-construct>
</web-app>